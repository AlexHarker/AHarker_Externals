<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<?xml-stylesheet href='./_c74_ref.xsl' type='text/xsl'?>
<c74object name="macaddress" module="AHarker Externals" category="Utility">
  <digest>
      Return the MAC address of any wireless interface
  </digest>
  <description>
      <o>macaddress</o> is an object for obtaining the MAC address of any wireless interface to provide a unique identifier for the host computer.
  </description>
  <objarglist>
      <objarg name="interface-type" optional="1" type="symbol">
          <digest>
              Set the interface type
          </digest>
          <description>
              The first argument sets the interface type for macaddress.
              <br/>
              <br/>
              The options are:
              <br/>
              <br/>
              <m>any</m>
              <br/>
              <br/>
              <m>wifi</m>
              <br/>
              <br/>
              <m>ethernet</m>
              <br/>
              <br/>
              a network interface name (e.g. <m>en0</m>).
          </description>
      </objarg>
  </objarglist>
  <methodlist>
      <method name="bang">
          <arglist>
              <arg name="bang" type="bang" />
          </arglist>
          <digest>
              Output the MAC address
          </digest>
          <description>
              A <m>bang</m> will cause <o>macaddress</o> to output the MAC address.
          </description>
      </method>
      <method name="set">
          <arglist>
              <arg name="set" type="symbol" />
          </arglist>
          <digest>
              Set the interface type
          </digest>
          <description>
              The <m>set</m> message followed by a symbol will change the interface type and bang an output.
              <br/>
              <br/>
              The options are:
              <br/>
              <br/>
              <m>set any</m>
              <br/>
              <br/>
              <m>set wifi</m>
              <br/>
              <br/>
              <m>set ethernet</m>
              <br/>
              <br/>
              <m>set</m> a network interface name (e.g. <m>en0</m>).
          </description>
      </method>
  </methodlist>
  <attributelist>
  </attributelist>
  <misc name="Output">
    <entry name="symbol">
        <description>
            Outputs the MAC address
        </description>
    </entry>
  </misc>
  <seealsolist>
  </seealsolist>
  <misc name="Discussion">
    <entry name="Keywords">
      <description>utility</description>
    </entry>
  </misc>
</c74object>
